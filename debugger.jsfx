desc: MIDI debugger

in_pin:none
out_pin:none

slider1:0<0,128,1>offset
slider2:0<0,127,1>status hi
slider3:0<0,127,1>status lo
slider4:0<0,127,1>data1
slider5:0<0,127,1>data2
slider6:0<0,0,1>messages received

import gianni-lib.jsfx-inc

@block

while (msg.recv())
(
  slider1 = msg.offset;
  slider2 = msg.statusHi;
  slider3 = msg.statusLo;
  slider4 = msg.data1;
  slider5 = msg.data2;

  msg.send();

  slider6 += 1;
);
